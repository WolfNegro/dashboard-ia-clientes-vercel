{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/(favicon\\.ico|.*\\.(css|js|png|jpg|svg|woff2?))",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    { "src": "/(.*)", "dest": "api/index.py" }
  ]
}
